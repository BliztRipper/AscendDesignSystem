extends _template
block body
  nav
    .navGrid
      .navGrid__offMenu
        img.img-fluid(src="images/Menu-new.png" alt="Mobile Menu", srcset="")
      .navGrid__logo
        img.img-fluid(alt="Logo" src="images/logo.png" srcset="images/logo.png 1x, images/logo@2x.png 2x")
      .navGrid__about
        span About Us
  section.hero
    .container
      .col-12
        header.header
          h1.header__title(data-aos="fade-up") Design
          h2.header__subtitle(data-aos="fade-down")  Sprint
        img.img-fluid.header__image(data-aos="zoom-in" alt="Logo" src="images/home.png" srcset="images/home.png 1x, images/home@2x.png 2x")  
        button.btn.btn-link.header__button.mx-auto.d-block(data-aos="fade-right" data-aos-offset='0' data-aos-delay='500') Discover
  //- section.main
    
    