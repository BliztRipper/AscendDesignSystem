extends _template
block body
  -var ui = 'active';
  include _nav
  section.interface.animated.fadeInRight
    .container
      .col-12
        .empathize__intro(data-aos="fade-right" data-aos-delay='400') DAY 3      
      .empathize__grid-content
        .empathize__grid-content--col-1
            img.interface__image(src='images/ui-1.png' data-aos="fade-right" data-aos-delay='800')
            img.interface__image--sub(src='images/ui-2.png' data-aos="fade-up" data-aos-delay='950')
        .empathize__grid-content--col-2
          .empathize__grid-content__inner-grid 
            header.empathize__grid-content__inner-grid--row-1
              h1.empathize__title(data-aos="fade-left" data-aos-delay='600') IDEATION
              h2.empathize__title-ex(data-aos="fade-left" data-aos-delay='600') USER INTERFACE
              p.empathize__subtitle(data-aos="fade-up" data-aos-delay='600') Welcome to your resource for planning <br/>and running a Google design sprint.
          .empathize__grid-content__inner-grid--row-2 
            .swipeDown Swipe Down for More   
    .fixed-bottom.explore#triggerEmpathize(data-aos="fade-zoom-in" data-aos-offset='0' data-aos-delay='800')  User Interface
  .gotoDay4 Next  
  .backto2 Back 